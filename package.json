{
  "name": "snyk-swiftpm-plugin",
  "description": "snyk cli SwiftPm Plugin",
  "main": "dist/index.js",
  "scripts": {
    "format:check": "prettier --check '{lib,test}/**/*.{js,ts,json,yml}'",
    "format": "prettier --write '{lib,test}/**/*.{js,ts,json,yml}'",
    "lint": "npm run format:check && npm run lint:eslint",
    "lint:eslint": "eslint --color --cache 'lib/**/*.{js,ts}'",
    "test": "npm run test:unit",
    "test:unit": "jest --coverage",
    "test:watch": "tsc-watch --onSuccess 'npm run test:unit'",
    "build": "tsc",
    "build-watch": "tsc -w",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snyk/snyk-swiftpm-plugin.git"
  },
  "author": "snyk.io",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/snyk/snyk-swiftpm-plugin/issues"
  },
  "homepage": "https://github.com/snyk/snyk-swiftpm-plugin#readme",
  "dependencies": {
    "@snyk/dep-graph": "^2.3.0",
    "lookpath": "^1.2.2",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^14",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^28.1.3",
    "prettier": "^2.3.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.0.0",
    "tsc-watch": "^4.4.0",
    "typescript": "^4.3.2"
  }
}
